    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.1",
    "@types/webpack-env": "^1.16.3",
    "@types/webpackbar": "^4.0.3",
